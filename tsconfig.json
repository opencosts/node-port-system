{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "traceResolution": true,
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "types": ["jsonwebtoken", "express"]  // Add express here
  },
   "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}